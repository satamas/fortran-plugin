// R453
derived_type_spec ::= fidentifier ('(' <<list type_param_spec>> ')')? { pin = 1 }
// R454
type_param_spec ::= (fidentifier '=')? type_param_value
// R455 is in clause 7
// R456
component_spec ::= (fidentifier '=')? component_data_source
// R457
