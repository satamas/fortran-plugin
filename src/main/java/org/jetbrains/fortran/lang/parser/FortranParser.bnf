// R1215 is trivial
// R1216
proc_pointer_init ::= function_reference | fidentifier
// R1217 is trivial
// R1218
intrinsic_stmt ::= label_decl? intrinsic_ ('::')? <<list fidentifier>> { pin = 2 }
// R1219
function_reference ::= procedure_designator '(' <<list actual_arg_spec>>? ')' { pin = 4; extends=expr }
// R1220
call_stmt ::= label_decl? call_ procedure_designator ('(' <<list actual_arg_spec>>? ')')? { pin = 2 }
// R1221
procedure_designator ::= data_path
// R1222
actual_arg_spec ::= (fidentifier '=')? actual_arg
// 1223
actual_arg ::= expr | alt_return_spec // expr matches variable, procedure-name and proc-component-ref
// 1224 deprecated
alt_return_spec ::= '*' label
// R1225
prefix ::= prefix_spec+ { pin = 1 }
// R1226
prefix_spec ::= declaration_type_spec | elemental_ | impure_ | module_ | pure_ | recursive_
// R1227
function_subprogram ::= function_stmt eol+ block internal_subprogram_part? end_function_stmt { pin = 1; extends = program_unit }
// R1228
function_stmt ::= label_decl? prefix? function_ fidentifier '(' <<list fidentifier>>? ')' suffix? { pin = 3 }
// R1229 and R1230 are trivial
// R1231
suffix ::= (language_binding_spec (result_ '(' fidentifier ')')?) | (result_ '(' fidentifier ')' (language_binding_spec)?)
// R1232
end_function_stmt ::= label_decl? ((end_ (function_ fidentifier?)?) | (endfunction_ fidentifier?)) { pin = 2 }
// R1233
subroutine_subprogram ::= subroutine_stmt eol+ block internal_subprogram_part? end_subroutine_stmt { pin = 1; extends = program_unit }
// R1234
subroutine_stmt ::= label_decl? prefix? subroutine_ fidentifier ('(' <<list dummy_arg>>? ')' language_binding_spec? )?  { pin = 3 }
// R1235
dummy_arg ::= '*' | fidentifier
// R1236
end_subroutine_stmt ::= label_decl? ((end_ (subroutine_ fidentifier?)?) | (endsubroutine_ fidentifier?)) { pin = 2 }
// R1237
separate_module_subprogram ::= mp_subprogram_stmt eol+ block internal_subprogram_part? end_mp_subprogram_stmt { pin = 1; extends = program_unit }
// R1238
mp_subprogram_stmt ::= label_decl? module_ procedure_ fidentifier { pin = 3 }
// R1239
end_mp_subprogram_stmt ::= label_decl? ((end_ (procedure_ fidentifier?)?) | (endprocedure_ fidentifier?)) { pin = 2}
// R1240
entry_stmt ::= label_decl? entry_ fidentifier ('(' <<list dummy_arg>>? ')' suffix? )? { pin = 2 }
// R1241
return_stmt ::= label_decl? return_ (expr)? { pin = 2}
// R1242
contains_stmt ::= label_decl? contains_  { pin = 2 }
// R1243
stmt_function_stmt ::= label_decl? fidentifier '(' <<list fidentifier>>? ')' '=' expr { pin = 6 }

// deleted statements
pause_stmt ::= label_decl? pause_ (expr)? { pin = 2 }
assign_stmt ::= label_decl? assign_ label to_ variable { pin = 2 }
assigned_goto_stmt ::= label_decl? (goto_ | (go_ to_)) fidentifier (','? '(' <<list label_decl>> ')' )?  { pin = 3 }
include_stmt ::= label_decl? include_ string_literal { pin = 2 }

// nonstandard
accept_stmt ::= label_decl? accept_  format (',' <<list input_item>>)?  {pin = 2}
encode_stmt ::= label_decl? encode_ '(' expr ',' format ',' designator (',' <<list parameters_spec>>)?')' <<list output_item>> { pin = 2}
decode_stmt ::= label_decl? decode_ '(' expr ',' format ',' designator (',' <<list parameters_spec>>)?')' <<list input_item>> { pin = 2 }
type_stmt ::= label_decl? type_ format (','? <<list output_item>>?) { pin = 4 }